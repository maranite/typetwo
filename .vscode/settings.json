{
    "files.exclude": {
        "**/dist": true,
        "**/.eggs": true,
        "**/.virtualenv": true,
        "**/typetwo.egg-info": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.git": false,
        "**/.git/hooks": false,
        "**/.git/hooks/*.sample": true,
        "**/.git/{info,refs,logs,objects,config,COMMIT_EDITMSG,description,FETCH_HEAD,HEAD,index,ORIG_HEAD}": true
    },
    "python.autoComplete.extraPaths": [
        "src/typetwo"
    ],
    "python.analysis.extraPaths": [
        "src/typetwo"
    ],
    "python.defaultInterpreterPath": "${workspaceFolder}\\.virtualenv\\",
    "python.formatting.provider": "black",
    "python.testing.unittestArgs": [
        "-v",    
        "-s",
        "${workspaceFolder}",
        "-p",
        "test*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true
}