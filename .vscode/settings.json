{
    "files.exclude": {
        "**/dist": true,
        "**/.eggs": true,
        "**/.virtualenv": true,
        "**/typetwo.egg-info": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".git/hooks": false,
        ".git/*HEAD": true,
        ".git/index": true,
        ".git/description": true,
        ".git/COMMIT*": true,
        ".git/config": true,
        ".git/info": true,
        ".git/logs": true,
        ".git/objects": true,
        ".git/refs": true
    },
    "python.autoComplete.extraPaths": [
        "src/typetwo"
    ],
    "python.analysis.extraPaths": [
        "src/typetwo"
    ],
    "python.defaultInterpreterPath": "${workspaceFolder}\\.virtualenv\\",
    "python.formatting.provider": "black",
    "python.testing.unittestArgs": [
        "-v",    
        "-s",
        "${workspaceFolder}",
        "-p",
        "test*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true
}